{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/index.js" },
    { "source": "/(.*)", "destination": "/public/$1" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
